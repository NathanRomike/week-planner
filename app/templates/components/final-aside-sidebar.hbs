<div class="content">

  <section class="final">
    <header class="final__header">
      <h1>Date Night</h1>
      <h3>Here is how your night is looking.</h3>
    </header>
    {{final-map showFinalMap="showFinalMap"}}

  </section>

  <aside class="aside">
    <div class="aside__final">
      <h1>Date Night</h1>
      <p>
        Here's your itinerary...
      </p>
      <img class="aside__header__icon" src="img/icon.png">
    </div>
    {{log "These are the places stored in googleMaps"}}
    {{log this.googleMap.art}}
    {{log googleMap.dinner}}
    <div class="aside__content">
      <h1 class="category"><i class="fa fa-glass"></i>Your choices</h1>
      {{log selectedItems.drink}}
      <div class="aside__drop">
        {{final-aside-card business=googleMap.drink}}
      </div>
      <h1 class="category"><i class="fa fa-cutlery"></i></h1>
      <div class="aside__drop">
        {{final-aside-card business=googleMap.dinner}}
      </div>
      <h1 class="category"><i class="fa fa-users"></i></h1>
      <div class="aside__drop">
        {{final-aside-card business=googleMap.art}}
      </div>
    </div>
  </aside>

</div>
